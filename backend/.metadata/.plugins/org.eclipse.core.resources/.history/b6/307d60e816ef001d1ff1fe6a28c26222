package com.cg.authservice.service;

import com.cg.authservice.dto.JwtRequest;
import com.cg.authservice.dto.JwtResponse;
import com.cg.authservice.dto.UserDto;

public interface AuthService {
	JwtResponse signIn(JwtRequest request);

	Object register(UserDto dto);

	void signOut();
	
	Object validateToken(String token);
}
